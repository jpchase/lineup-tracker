{
  "name": "lineup-tracker",
  "private": true,
  "repository": "github:jpchase/lineup-tracker",
  "scripts": {
    "start": "npm run start:webpack",
    "start:esds": "tsc && es-dev-server",
    "start:webpack": "tsc && webpack-dev-server --mode development",
    "serve:tmp": "npm run build:static && firebase serve",
    "build": "tsc && npm run build:prpl-server && npm run build:static",
    "build:static": "tsc && polymer build",
    "build:webpack": "rm -rf dist && webpack --config webpack.config.js",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "polymer serve --port 3000 dist",
    "test": "npm run test:karma && npm run test:integration",
    "test:integration": "tsc && npm run build:webpack && mocha test/integration --timeout=15000",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=25000",
    "test:karma": "tsc && karma start --coverage",
    "test:karma_single": "tsc && karma start --coverage --grep test/reducers/live.test.js",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:compatibility": "karma start --compatibility all --coverage",
    "test:compatibility:watch": "karma start --compatibility all --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --compatibility all --coverage"
  },
  "eslintConfig": {
    "extends": "google"
  },
  "dependencies": {
    "@material/mwc-button": "^0.13.0",
    "@material/mwc-checkbox": "^0.13.0",
