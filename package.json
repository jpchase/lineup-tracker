{
  "name": "lineup-tracker",
  "private": true,
  "repository": "github:jpchase/lineup-tracker",
  "scripts": {
    "start": "gulp serve",
    "serve:tmp": "npm run build:static && firebase serve",
    "build": "tsc && npm run build:prpl-server && npm run build:static",
    "build:static": "tsc && polymer build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit_coverage && npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=15000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:unit_single": "tsc && jest test/models/clock.spec.ts",
    "test:unit_redux": "tsc && jest",
    "test:unit_coverage": "tsc && jest --coverage",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=25000"
  },
  "eslintConfig": {
    "extends": "google"
  },
  "dependencies": {
    "@material/mwc-button": "^0.5.0",
    "@material/mwc-checkbox": "^0.5.0",
    "@material/mwc-fab": "^0.5.0",
    "@material/mwc-formfield": "^0.5.0",
    "@material/mwc-icon": "^0.5.0",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.0.1",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "firebase": "^5.7.2",
    "idb-keyval": "^3.1.0",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.1.6",
    "@types/jest": "^23.3.10",
    "axe-core": "^3.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "mock-cloud-firestore": "^0.9.2",
    "pixelmatch": "^4.0.2",
    "polymer-build": "^3.1.1",
    "polymer-cli": "^1.9.5",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.2.0",
    "puppeteer": "^1.5.0",
    "superstatic": "^6.0.4",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3333",
    "wct-browser-legacy": "^1.0.1"
  },
  "localDependencies": {
    "@material/mwc-select": "../../material-components-web-components/packages/select",
    "@material/mwc-list": "../../material-components-web-components/packages/list",
    "@material/mwc-menu": "../../material-components-web-components/packages/menu"
  }
}
