    "@polymer/test-fixture": "^4.0.2",
    "@types/jest": "^23.3.10",
    "axe-core": "^3.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "del": "^3.0.0",
    "fake-indexeddb": "^2.1.0",
    "gulp": "^4.0.0",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mocha": "^5.2.0",
    "mock-cloud-firestore": "^0.9.2",
    "pixelmatch": "^4.0.2",
    "polymer-build": "^3.1.1",
    "polymer-cli": "^1.9.5",
    "polyserve": "^0.27.0",
