{
  "name": "lineup-tracker",
  "private": true,
  "repository": "github:jpchase/lineup-tracker",
  "scripts": {
    "env:copy": "cp src/env-config.$npm_config_envname.json src/env-config.json",
    "env:copy_dev": "npm run env:copy --envname=dev",
    "env:copy_test": "npm run env:copy --envname=test",
    "env:copy_staging": "npm run env:copy --envname=staging",
    "lint": "eslint . && prettier \"**/*.ts\" --check --require-pragma",
    "format": "eslint --ext .ts,.html . --fix && prettier \"**/*.ts\" --write --insert-pragma",
    "start": "npm run start:webds",
    "start:webds": "npm run env:copy_dev && tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"web-dev-server\"",
    "start:offline": "npm run env:copy_dev && tsc && web-dev-server --offline",
    "build:deploy": "tsc && npm run env:copy_staging && npm run build:webpack",
    "build:test": "tsc && npm run env:copy_test && npm run build:webpack",
    "build:webpack": "tsc && rm -rf dist && webpack --config webpack.config.js",
    "serve:test": "firebase emulators:start --import test/integration/data/firebase/",
    "serve:webpack": "web-dev-server --port 3000 --root-dir dist --app-index dist/index.html",
    "test": "npm run lint && npm run test:unit_full && npm run test:integration",
    "test:integration": "npm run build:test && npm run test:integration_impl",
    "test:integration_impl": "firebase emulators:exec \"mocha --config config/mocha-integration.cjs\" --project demo-integration --import test/integration/data/firebase",
    "test:integration_single": "tsc && npm run env:copy_test && npm run build:webpack && npm run test:integration_single_impl",
    "test:integration_single_impl": "firebase emulators:exec \"mocha test/integration/functional-live.js --timeout=60000\" --project demo-integration --import test/integration/data/firebase",
    "test:start_emulators": "firebase emulators:start --project demo-integration --import test/integration/data/firebase",
    "test:integration_single_direct": "mocha test/integration/functional-game.js --timeout=25000",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && npm run env:copy_test && npm run build:webpack && npm run test:regenerate_screenshots_impl",
    "test:regenerate_screenshots_impl": "firebase emulators:exec \"mocha test/integration/screenshots-baseline/regenerate.js --timeout=25000\" --import test/integration/data/firebase",
    "test:unit_full": "tsc && npm run env:copy_dev && web-test-runner --concurrency 1 --coverage --group all --config config/web-test-runner.config.mjs",
    "test:storage": "tsc && npm run env:copy_dev && web-test-runner --group storage --config config/web-test-runner.config.mjs",
    "test:unit": "tsc && npm run env:copy_dev && web-test-runner --group unit --config config/web-test-runner.config.mjs",
    "test:unit_single": "tsc && npm run env:copy_dev && web-test-runner --group single --config config/web-test-runner.config.mjs",
    "test:unit_single_watch": "tsc && npm run env:copy_dev && web-test-runner --watch --group single --config config/web-test-runner.config.mjs",
    "lhci:local": "lhci collect --config=./config/lighthouserc.js"
  },
  "dependencies": {
    "@lit-labs/router": "^0.1.3",
    "@lit/context": "^1.1.0",
    "@material/mwc-button": "^0.27.0",
    "@material/mwc-checkbox": "^0.27.0",
    "@material/mwc-circular-progress": "^0.27.0",
    "@material/mwc-dialog": "^0.27.0",
    "@material/mwc-drawer": "^0.27.0",
    "@material/mwc-fab": "^0.27.0",
    "@material/mwc-formfield": "^0.27.0",
    "@material/mwc-icon": "^0.27.0",
    "@material/mwc-icon-button": "^0.27.0",
    "@material/mwc-icon-button-toggle": "^0.27.0",
    "@material/mwc-linear-progress": "^0.27.0",
    "@material/mwc-list": "^0.27.0",
    "@material/mwc-menu": "^0.27.0",
    "@material/mwc-radio": "^0.27.0",
    "@material/mwc-select": "^0.27.0",
    "@material/mwc-snackbar": "^0.27.0",
    "@material/mwc-tab": "^0.27.0",
    "@material/mwc-tab-bar": "^0.27.0",
    "@material/mwc-textfield": "^0.27.0",
    "@material/mwc-top-app-bar": "^0.27.0",
    "@material/mwc-top-app-bar-fixed": "^0.27.0",
    "@reduxjs/toolkit": "^2.0.1",
    "debug": "^4.4.0",
    "firebase": "^11.3.0",
    "idb-keyval": "^6.2.0",
    "lit": "^2.7.6",
    "pwa-helpers": "^0.9.0",
    "redux-persist": "^6.0.0",
    "ts-loader": "^9.4.4"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.1",
    "@babel/core": "^7.24.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-attributes": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@blockquote/test-runner-mocha-style-reporter": "^1.5.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@lhci/cli": "^0.12.0",
    "@open-wc/eslint-config": "^12.0.3",
    "@open-wc/testing": "^4.0.0",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/sinon-chai": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-rollup": "^0.6.4",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-puppeteer": "^0.17.0",
    "axe-core": "^4.9.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^5.1.2",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^9.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-lit": "^1.15.0",
    "firebase-admin": "^13.1.0",
    "firebase-tools": "^13.31.1",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.0.1",
    "mock-cloud-firestore": "^0.12.0",
    "pixelmatch": "^5.3.0",
    "prettier": "^3.4.2",
    "puppeteer": "^24.1.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "typescript": "^5.3.3",
    "urlpattern-polyfill": "^8.0.2",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  },
  "prettier": {
    "insertPragma": true,
    "singleQuote": true
  }
}
