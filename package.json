{
  "name": "lineup-tracker",
  "private": true,
  "repository": "github:jpchase/lineup-tracker",
  "scripts": {
    "start": "npm run start:webds",
    "start:webds": "tsc && web-dev-server",
    "serve:tmp": "npm run build:static && firebase serve",
    "build": "tsc && npm run build:prpl-server && npm run build:static",
    "build:static": "tsc && polymer build",
    "build:webpack": "tsc && rm -rf dist && webpack --config webpack.config.js",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "serve:webpack": "web-dev-server --port 3000 --root-dir dist --app-index dist/index.html",
    "test": "npm run test:unit_full && npm run test:integration",
    "test:integration": "tsc && npm run build:webpack && npm run test:integration_impl",
    "test:integration_impl": "firebase emulators:exec \"mocha test/integration --timeout=15000\" --import test/integration/data/firebase",
    "test:integration_single": "tsc && npm run build:webpack && npm run test:integration_single_impl",
    "test:integration_single_impl": "firebase emulators:exec \"mocha test/integration/visual.js --timeout=15000\" --import test/integration/data/firebase",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && npm run build:webpack && npm run test:regenerate_screenshots_impl",
    "test:regenerate_screenshots_impl": "firebase emulators:exec \"mocha test/integration/screenshots-baseline/regenerate.js --timeout=25000\" --import test/integration/data/firebase",
    "test:unit-karma": "tsc && karma start --coverage",
    "test:unit_full": "tsc && web-test-runner --coverage \"test/unit/**/*.test.js\" --config config/web-test-runner.config.mjs",
    "test:unit": "tsc && web-test-runner \"test/unit/**/*.test.js\" --config config/web-test-runner.config.mjs",
    "test:unit-single": "tsc && karma start --coverage --grep test/unit/components/lineup-roster.test.js",
    "test:unit_single": "tsc && web-test-runner \"test/unit/slices/game/game-slice.test.js\" --config config/web-test-runner.config.mjs",
    "test:unit-watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:compatibility": "karma start --compatibility all --coverage",
    "test:compatibility:watch": "karma start --compatibility all --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --compatibility all --coverage"
  },
  "eslintConfig": {
    "extends": "google"
  },
  "dependencies": {
    "@material/mwc-button": "^0.25.3",
    "@material/mwc-checkbox": "^0.25.3",
    "@material/mwc-circular-progress": "^0.25.3",
    "@material/mwc-dialog": "^0.25.3",
    "@material/mwc-drawer": "^0.25.3",
    "@material/mwc-fab": "^0.25.3",
    "@material/mwc-formfield": "^0.25.3",
    "@material/mwc-icon": "^0.25.3",
    "@material/mwc-icon-button-toggle": "^0.25.3",
    "@material/mwc-linear-progress": "^0.25.3",
    "@material/mwc-list": "^0.25.3",
    "@material/mwc-menu": "^0.25.3",
    "@material/mwc-select": "^0.25.3",
    "@material/mwc-snackbar": "^0.25.3",
    "@material/mwc-tab": "^0.25.3",
    "@material/mwc-tab-bar": "^0.25.3",
    "@material/mwc-textfield": "^0.25.3",
    "@material/mwc-top-app-bar": "^0.25.3",
    "@material/mwc-top-app-bar-fixed": "^0.25.3",
    "@polymer/polymer": "^3.3.1",
    "@reduxjs/toolkit": "^1.6.2",
    "debug": "^4.3.2",
    "firebase": "^9.6.10",
    "idb-keyval": "^6.1.0",
    "immer": "^9.0.12",
    "lit": "^2.0.2",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "ts-loader": "^9.2.6",
    "web-animations-js": "^2.3.2"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.3.1",
    "@babel/core": "^7.17.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@blockquote/test-runner-mocha-style-reporter": "^1.1.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/testing": "^3.1.2",
    "@open-wc/testing-karma": "^4.0.9",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.0.0",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/sinon-chai": "^3.2.8",
    "@web/dev-server": "^0.1.30",
    "@web/test-runner": "^0.13.21",
    "@web/test-runner-puppeteer": "^0.10.3",
    "axe-core": "^4.3.5",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "copy-webpack-plugin": "^10.2.4",
    "deepmerge": "^4.2.2",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-lit": "^1.6.1",
    "firebase-tools": "^9.22.0",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^9.2.2",
    "mock-cloud-firestore": "^0.12.0",
    "pixelmatch": "^5.2.1",
    "puppeteer": "^11.0.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.5.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
