{
  "name": "lineup-tracker",
  "private": true,
  "repository": "github:jpchase/lineup-tracker",
  "scripts": {
    "env:copy": "cp src/env-config.$npm_config_envname.json src/env-config.json",
    "env:copy_dev": "npm run env:copy --envname=dev",
    "env:copy_test": "npm run env:copy --envname=test",
    "env:copy_staging": "npm run env:copy --envname=staging",
    "start": "npm run start:webds",
    "start:webds": "tsc && npm run env:copy_dev && tsc && web-dev-server",
    "serve:tmp": "npm run build:static && firebase serve",
    "build:deploy": "tsc && npm run env:copy_staging && npm run build:webpack",
    "build:webpack": "tsc && rm -rf dist && webpack --config webpack.config.js",
    "serve:webpack": "web-dev-server --port 3000 --root-dir dist --app-index dist/index.html",
    "test": "npm run test:unit_full && npm run test:integration",
    "test:integration": "tsc && npm run env:copy_test && npm run build:webpack && npm run test:integration_impl",
    "test:integration_impl": "firebase emulators:exec \"mocha --config config/mocha-integration.cjs\" --import test/integration/data/firebase",
    "test:integration_single": "tsc && npm run env:copy_test && npm run build:webpack && npm run test:integration_single_impl",
    "test:integration_single_impl": "firebase emulators:exec \"mocha test/integration/functional-game.js --timeout=25000\" --project resplendent-fire-4542 --import test/integration/data/firebase",
    "test:integration_single_direct": "mocha test/integration/functional-game.js --timeout=15000",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && npm run env:copy_test && npm run build:webpack && npm run test:regenerate_screenshots_impl",
    "test:regenerate_screenshots_impl": "firebase emulators:exec \"mocha test/integration/screenshots-baseline/regenerate.js --timeout=25000\" --import test/integration/data/firebase",
    "test:unit_full": "tsc && npm run env:copy_dev && web-test-runner --coverage --group all --config config/web-test-runner.config.mjs",
    "test:storage": "tsc && npm run env:copy_dev && web-test-runner --group storage --config config/web-test-runner.config.mjs",
    "test:unit": "tsc && npm run env:copy_dev && web-test-runner --group unit --config config/web-test-runner.config.mjs",
    "test:unit_single": "tsc && npm run env:copy_dev && web-test-runner --group single --config config/web-test-runner.config.mjs",
    "test:unit_single_watch": "tsc && npm run env:copy_dev && web-test-runner --watch --group single --config config/web-test-runner.config.mjs"
  },
  "eslintConfig": {
    "extends": "google"
  },
  "dependencies": {
    "@lit-labs/context": "^0.1.3",
    "@material/mwc-button": "^0.26.1",
    "@material/mwc-checkbox": "^0.26.1",
    "@material/mwc-circular-progress": "^0.26.1",
    "@material/mwc-dialog": "^0.26.1",
    "@material/mwc-drawer": "^0.26.1",
    "@material/mwc-fab": "^0.26.1",
    "@material/mwc-formfield": "^0.26.1",
    "@material/mwc-icon": "^0.26.1",
    "@material/mwc-icon-button-toggle": "^0.26.1",
    "@material/mwc-linear-progress": "^0.26.1",
    "@material/mwc-list": "^0.26.1",
    "@material/mwc-menu": "^0.26.1",
    "@material/mwc-select": "^0.26.1",
    "@material/mwc-snackbar": "^0.26.1",
    "@material/mwc-tab": "^0.26.1",
    "@material/mwc-tab-bar": "^0.26.1",
    "@material/mwc-textfield": "^0.26.1",
    "@material/mwc-top-app-bar": "^0.26.1",
    "@material/mwc-top-app-bar-fixed": "^0.26.1",
    "@polymer/polymer": "^3.3.1",
    "@reduxjs/toolkit": "^1.8.3",
    "debug": "^4.3.4",
    "firebase": "^9.9.0",
    "idb-keyval": "^6.2.0",
    "immer": "^9.0.12",
    "lit": "^2.3.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "reselect": "^4.1.5",
    "ts-loader": "^9.3.1"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.4.4-alpha.298",
    "@babel/core": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@blockquote/test-runner-mocha-style-reporter": "^1.4.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/testing": "^3.1.6",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.1.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/sinon-chai": "^3.2.8",
    "@web/dev-server": "^0.1.34",
    "@web/test-runner": "^0.13.31",
    "@web/test-runner-puppeteer": "^0.10.5",
    "axe-core": "^4.4.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "copy-webpack-plugin": "^10.2.4",
    "deepmerge": "^4.2.2",
    "eslint": "^8.22.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-lit": "^1.6.1",
    "firebase-tools": "^11.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^10.0.0",
    "mochawesome": "^7.0.1",
    "mock-cloud-firestore": "^0.12.0",
    "pixelmatch": "^5.3.0",
    "puppeteer": "^17.1.3",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
